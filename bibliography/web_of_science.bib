ORCID-Numbers = {Travis, Justin/0000-0002-5785-4272
   Mustin, Karen/0000-0002-2828-2316
   Mustin, Karen/0000-0002-2828-2316
   Benton, Tim G/0000-0002-7448-1973},
Unique-ID = {WOS:000320327500015},
}

@article{ WOS:000320500000025,
Author = {Wroe, Stephen and Field, Judith H. and Archer, Michael and Grayson,
   Donald K. and Price, Gilbert J. and Louys, Julien and Faith, J. Tyler
   and Webb, Gregory E. and Davidson, Iain and Mooney, Scott D.},
Title = {Climate change frames debate over the extinction of megafauna in Sahul
   (Pleistocene Australia-New Guinea)},
Journal = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA},
Year = {2013},
Volume = {110},
Number = {22},
Pages = {8777-8781},
Month = {MAY 28},
Abstract = {Around 88 large vertebrate taxa disappeared from Sahul sometime during
   the Pleistocene, with the majority of losses (54 taxa) clearly taking
   place within the last 400,000 years. The largest was the 2.8-ton
   browsing Diprotodon optatum, whereas the similar to 100- to 130-kg
   marsupial lion, Thylacoleo carnifex, the world's most specialized
   mammalian carnivore, and Varanus priscus, the largest lizard known, were
   formidable predators. Explanations for these extinctions have centered
   on climatic change or human activities. Here, we review the evidence and
   arguments for both. Human involvement in the disappearance of some
   species remains possible but unproven. Mounting evidence points to the
   loss of most species before the peopling of Sahul (circa 50-45 ka) and a
   significant role for climate change in the disappearance of the
   continent's megafauna.},
DOI = {10.1073/pnas.1302698110},
