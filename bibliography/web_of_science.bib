@article{ WOS:000543974800026,
Author = {Seersholm, Frederik V. and Werndly, Daniel J. and Grealy, Alicia and
   Johnson, Taryn and Keenan Early, Erin M. and Lundelius, Ernest L. and
   Winsborough, Barbara and Farr, Grayal Earle and Toomey, Rickard and
   Hansen, Anders J. and Shapiro, Beth and Waters, Michael R. and McDonald,
   Gregory and Linderholm, Anna and Stafford, Jr., Thomas W. and Bunce,
   Michael},
Title = {Rapid range shifts and megafaunal extinctions associated with late
   Pleistocene climate change},
Journal = {NATURE COMMUNICATIONS},
Year = {2020},
Volume = {11},
Number = {1},
Month = {JUN 2},
Abstract = {Large-scale changes in global climate at the end of the Pleistocene
   significantly impacted ecosystems across North America. However, the
   pace and scale of biotic turnover in response to both the Younger Dryas
   cold period and subsequent Holocene rapid warming have been challenging
   to assess because of the scarcity of well dated fossil and pollen
   records that covers this period. Here we present an ancient DNA record
   from Hall's Cave, Texas, that documents 100 vertebrate and 45 plant taxa
   from bulk fossils and sediment. We show that local plant and animal
   diversity dropped markedly during Younger Dryas cooling, but while plant
   diversity recovered in the early Holocene, animal diversity did not.
   Instead, five extant and nine extinct large bodied animals disappeared
   from the region at the end of the Pleistocene. Our findings suggest that
   climate change affected the local ecosystem in Texas over the
   Pleistocene-Holocene boundary, but climate change on its own may not
   explain the disappearance of the megafauna at the end of the
   Pleistocene.},
DOI = {10.1038/s41467-020-16502-3},
