Title = {Time-dependent measurements of flame temperature and the OH radical in
   the unsteady extinction of non-premixed flames},
Journal = {COMBUSTION AND FLAME},
Year = {2005},
Volume = {141},
Number = {1-2},
Pages = {186-190},
Month = {APR},
Abstract = {The extinction point, time-dependent flame temperature, relative {[}OH],
   and the instantaneous luminosity of a flame during the unsteady
   extinction process were measured in non-premixed counterflow flames, and
   characteristics of each parameter near the extinction limit were
   investigated. We found that the unsteady extinction point is much higher
